using Lab.Bank.Views.ManageUsers;
using Lab.Classes;

namespace lab
{
    class Program
    {
        static void Main()
        {
            p06(10);
        }
        static void p06(int height)
        {
            Console.WriteLine("*******");

            for (int i = 0; i < height; i++)
                Console.WriteLine("*     *");

            Console.WriteLine("*******");
        }
    }
}
