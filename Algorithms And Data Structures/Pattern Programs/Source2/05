using Lab;
using System;

class URI
{
    static void Main(string[] args)
    {
        p5(8);
    }
    static void p5(int rowsCount)
    {
        for (int i = 0; i < rowsCount; i++)
        {
            for (int j = 0; j < i; j++)
                Console.Write(' ');
            Console.WriteLine("********");
        }
    }
}
