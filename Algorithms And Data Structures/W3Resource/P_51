using Lab.Bank.Views.ManageUsers;

namespace lab
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(p51(new int[] { 1, 2, 5, 7, 110, 8 }));
        }
        static int p51(int[] numbers)
        {
            int maxNumber = 0;
            foreach (int number in numbers)
            {
                if (number > maxNumber)
                    maxNumber = number;

            }
            return maxNumber;
        }
    }
}
