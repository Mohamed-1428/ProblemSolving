using System;
using System.Collections.Generic;
using System.IO;
using System.Linq;

namespace lab
{
    class Program
    {
        static void Main()
        {
            printFinalResult(readAge());
        }
        static int readAge()
        {
            Console.Write("Enter your age: ");
            return int.Parse(Console.ReadLine());
        }
        static void printFinalResult(int age) =>
            Console.WriteLine("You look older than {0}", age);
    }
}
