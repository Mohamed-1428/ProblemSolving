using System;
using System.Collections.Generic;
using System.Linq;

namespace lab
{
    class Program
    {
        static void Main()
        {
            p25();
        }
        static bool isOdd(int number) => number % 2 != 0;
        static void p25()
        {
            for (int i = 1; i < 100; i++)
                if (isOdd(i))
                    Console.WriteLine(i);
        }
    }
}
