namespace lab
{
    class Program
    {
        static void Main(string[] args)
        {
            printNumberMultiplicationTable(readNumber());
        }
        static int readNumber()
        {
            Console.Write(" Enter the number: ");
            return int.Parse(Console.ReadLine());
        }
        static int multipleTwoNumbers(int number1, int number2) => number1 * number2;
        static void printNumberMultiplicationTable(int nummber)
        {
            for(int i=0;i<=10;i++)
                Console.WriteLine("5 * {0} = {1}",i, multipleTwoNumbers(nummber,i));
        }
    }
}
