using System;

class URI
{
    static void Main(string[] args)
    {
        Console.WriteLine(P33(15));
    }
    static bool isMultipleOfThree(int number) => number % 3 == 0;
    static bool isMultipleOfSeven(int number) => number % 7 == 0;

    static bool P33(int number) => isMultipleOfThree(number) || isMultipleOfSeven(number);
}
