using Lab.Bank.Views.ManageUsers;
using System.Text;

namespace lab
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(p48(new int[] { 1, 2, 2, 3, 3, 4, 5, 6, 5, 7, 7, 7, 8, 8, 1 }));
        }
        private static bool p48(int[] arr) => arr.Length > 1 && arr[0] == arr[arr.Length - 1];
    }
}
