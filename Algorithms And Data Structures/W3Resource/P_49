using Lab.Bank.Views.ManageUsers;
using System.Text;

namespace lab
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(P_49(new int[] { 1, 2, 2, 3, 3, 4, 5, 6, 5, 7, 7, 7, 8, 8, 1 }
                ,new int[] { 1, 2, 2, 3, 3, 4, 5, 6, 5, 7, 7, 7, 8, 8, 5 }));
        }
        static bool P_49(int[] arr1, int[] arr2) =>
            arr1[0] == arr2[0] || arr1[arr1.Length - 1] == arr2[arr2.Length - 1];
    }
}
