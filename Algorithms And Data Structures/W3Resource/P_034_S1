using System;

class URI
{
    static void Main(string[] args)
    {
        Console.WriteLine(StartWith("Hello how are you ?", "Hellos"));
    }
    static bool StartWith (string s, string startWith)
    {
        for (int i = 0; i < startWith.Length; i++)
            if (s[i] != startWith[i])
                return false;

        return true;
    }
    
}
