using System;
using System.Collections.Generic;

namespace lab
{
    class Program
    {
        static void Main()
        {
            Console.WriteLine(P_020(0,23));
        }
        static int abs(int number) => number > 0 ? number : number * -1;
        static int P_020(int number1, int number2)
        {
            int absolute = abs(number1 - number2);
            return number1 > number2 ? absolute * 2 : absolute;
        }
    }
}
