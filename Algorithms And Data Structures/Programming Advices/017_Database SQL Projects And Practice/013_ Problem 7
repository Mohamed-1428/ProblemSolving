select *,
       (cast(r1.VehicleNumber as float) /cast(r1.TotalVehicles as float)) as Percentage
	  
from(
select make, 
	count(*) VehicleNumber,
	(select count(*) from vehicleMasterDetails) TotalVehicles
	  
from vehicleMasterDetails
where Year between 1950 and 2000
group by make) r1

order by r1.Make
