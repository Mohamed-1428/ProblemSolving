 static bool isEvenNumber(int number) =>
            number % 2 == 0;
static int incrementIndexBy(int index) =>
            isEvenNumber(index) ? index : index+1;
static void printEvenNumbersInRange(int firstNumber, int lastNumber)
        {
            int inrementedIndex = incrementIndexBy(firstNumber);

            for(int i= inrementedIndex; i<=lastNumber;i+= inrementedIndex)
                    Console.WriteLine(i);
        }
