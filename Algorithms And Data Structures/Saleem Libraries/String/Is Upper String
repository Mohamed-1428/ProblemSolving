static char charToUpperCase(char c) =>
            (int)c >= 97 && (int)c <= 122 ? (char)((int)c - 32) : c;

static bool isUpperCase(string s)
        {
            for (int i = 0; i < s.Length; i++)
                if (s[i] != charToUpperCase(s[i]))
                    return false;
            return true;
        }
