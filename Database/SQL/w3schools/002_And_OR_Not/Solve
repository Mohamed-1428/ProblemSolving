select *
from customers t
where t.country='Germany' and t.city ='Berlin';

select *
from customers t
where t.city in ('Berlin','Munchen');

select *
from customers t
where t.country ='Berlin' or t.country = 'Spain';

select *
from customers t
where t.country !='Germany';

select *
from customers t
where t.country='Germany' and (t.City == "Berlin" OR t.City = 'Munchen');

SELECT * FROM Customers
WHERE NOT Country='Germany' AND NOT Country='USA';
