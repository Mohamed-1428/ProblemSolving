CREATE TABLE [ProjectDetailCalls] (
    [Id] int NOT NULL IDENTITY,
    [CallStartDate] datetime2 NOT NULL,
    [DurationInSeconds] int NOT NULL,
    [AddedOn] datetime2 NOT NULL,
    [ProjectDetailId] int NOT NULL,
    [EmployeeId] int NULL,
    CONSTRAINT [PK_ProjectDetailCalls] PRIMARY KEY ([Id]),
    CONSTRAINT [FK_ProjectDetailCalls_Employees_EmployeeId] FOREIGN KEY ([EmployeeId]) REFERENCES [Employees] ([Id]),
    CONSTRAINT [FK_ProjectDetailCalls_ProjectDetails_ProjectDetailId] FOREIGN KEY ([ProjectDetailId]) REFERENCES [ProjectDetails] ([Id]) ON DELETE CASCADE
);
GO

