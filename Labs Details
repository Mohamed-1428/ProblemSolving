CREATE TABLE [EmployeeWorkingHours] (
    [Id] int NOT NULL IDENTITY,
    [Day] datetime2 NOT NULL,
    [WorkingHours] float NOT NULL,
    [ProjectId] int NOT NULL,
    [EmployeeId] int NOT NULL,
    [SegmentName] nvarchar(450) NULL,
    CONSTRAINT [PK_EmployeeWorkingHours] PRIMARY KEY ([Id]),
    CONSTRAINT [FK_EmployeeWorkingHours_Employees_EmployeeId] FOREIGN KEY ([EmployeeId]) REFERENCES [Employees] ([Id]) ON DELETE CASCADE,
    CONSTRAINT [FK_EmployeeWorkingHours_Projects_ProjectId] FOREIGN KEY ([ProjectId]) REFERENCES [Projects] ([Id]) ON DELETE CASCADE,
    CONSTRAINT [FK_EmployeeWorkingHours_Segments_SegmentName] FOREIGN KEY ([SegmentName]) REFERENCES [Segments] ([Name])
);
GO

CREATE INDEX [IX_EmployeeWorkingHours_EmployeeId] ON [EmployeeWorkingHours] ([EmployeeId]);
GO

CREATE INDEX [IX_EmployeeWorkingHours_ProjectId] ON [EmployeeWorkingHours] ([ProjectId]);
GO

CREATE INDEX [IX_EmployeeWorkingHours_SegmentName] ON [EmployeeWorkingHours] ([SegmentName]);
GO

INSERT INTO [__EFMigrationsHistory] ([MigrationId], [ProductVersion])
VALUES (N'20240922062013_AddEmployeeWorkingHoursTable', N'8.0.2');
GO

COMMIT;
GO

