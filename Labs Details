select pr.id,pr.Name,pr.DateFrom,pr.DateTo,  pd.GSM, c.Name, e.id userId, e.UserName, p.CallStartDate, p.DurationInSeconds
from ProjectDetailCalls p
join ProjectDetails pd
on p.ProjectDetailId = pd.Id

join Projects pr
on pd.ProjectId = pr.Id

join Employees e
on pd.EmployeeId = e.Id

join CallStatuses c
on pd.CallStatusId = c.Id

where pd.IsDeleted = '0'
and pr.IsDeleted = '0'

and pr.Id = 9

order by 1,p.CallStartDate desc;
