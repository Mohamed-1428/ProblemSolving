BEGIN TRANSACTION;
GO

ALTER TABLE [ProjectDetailCalls] DROP CONSTRAINT [FK_ProjectDetailCalls_Employees_EmployeeId];
GO

DROP INDEX [IX_ProjectDetailCalls_EmployeeId] ON [ProjectDetailCalls];
GO

DECLARE @var0 sysname;
SELECT @var0 = [d].[name]
FROM [sys].[default_constraints] [d]
INNER JOIN [sys].[columns] [c] ON [d].[parent_column_id] = [c].[column_id] AND [d].[parent_object_id] = [c].[object_id]
WHERE ([d].[parent_object_id] = OBJECT_ID(N'[ProjectDetailCalls]') AND [c].[name] = N'EmployeeId');
IF @var0 IS NOT NULL EXEC(N'ALTER TABLE [ProjectDetailCalls] DROP CONSTRAINT [' + @var0 + '];');
ALTER TABLE [ProjectDetailCalls] DROP COLUMN [EmployeeId];
GO

INSERT INTO [__EFMigrationsHistory] ([MigrationId], [ProductVersion])
VALUES (N'20240729064416_UpdateProjectDetailCallTable2', N'8.0.2');
GO

COMMIT;
GO

