BEGIN TRANSACTION;
GO

ALTER TABLE [Employees] ADD [SyriatelId] int NOT NULL DEFAULT 0;
GO

ALTER TABLE [CallStatuses] ADD [IsClosed] bit NOT NULL DEFAULT CAST(0 AS bit);
GO

CREATE TABLE [EmployeeCalls] (
    [Id] int NOT NULL IDENTITY,
    [GSM] nvarchar(max) NOT NULL,
    [CallStartDate] datetime2 NOT NULL,
    [Duration] decimal(18,2) NOT NULL,
    [AddedOn] datetime2 NOT NULL,
    [EmployeeId] int NOT NULL,
    CONSTRAINT [PK_EmployeeCalls] PRIMARY KEY ([Id]),
    CONSTRAINT [FK_EmployeeCalls_Employees_EmployeeId] FOREIGN KEY ([EmployeeId]) REFERENCES [Employees] ([Id]) ON DELETE CASCADE
);
GO

CREATE INDEX [IX_EmployeeCalls_EmployeeId] ON [EmployeeCalls] ([EmployeeId]);
GO

INSERT INTO [__EFMigrationsHistory] ([MigrationId], [ProductVersion])
VALUES (N'20240717091014_AddEmployeeCallsTable', N'8.0.2');
GO

COMMIT;
GO

BEGIN TRANSACTION;
GO

DECLARE @var0 sysname;
SELECT @var0 = [d].[name]
FROM [sys].[default_constraints] [d]
INNER JOIN [sys].[columns] [c] ON [d].[parent_column_id] = [c].[column_id] AND [d].[parent_object_id] = [c].[object_id]
WHERE ([d].[parent_object_id] = OBJECT_ID(N'[EmployeeCalls]') AND [c].[name] = N'Duration');
IF @var0 IS NOT NULL EXEC(N'ALTER TABLE [EmployeeCalls] DROP CONSTRAINT [' + @var0 + '];');
ALTER TABLE [EmployeeCalls] ALTER COLUMN [Duration] int NOT NULL;
GO

INSERT INTO [__EFMigrationsHistory] ([MigrationId], [ProductVersion])
VALUES (N'20240717101310_UpdateDurationToInt', N'8.0.2');
GO

COMMIT;
GO

