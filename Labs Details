CREATE TABLE [TelemarketerCalls] (
    [Id] int NOT NULL IDENTITY,
    [GSM] nvarchar(max) NOT NULL,
    [CallStartDate] datetime2 NOT NULL,
    [DurationInSeconds] int NOT NULL,
    [AddedOn] datetime2 NOT NULL,
    [EmployeeId] int NOT NULL,
    [ProjectDetailId] int NOT NULL,
    CONSTRAINT [PK_TelemarketerCalls] PRIMARY KEY ([Id]),
    CONSTRAINT [FK_TelemarketerCalls_Employees_EmployeeId] FOREIGN KEY ([EmployeeId]) REFERENCES [Employees] ([Id]),
    CONSTRAINT [FK_TelemarketerCalls_ProjectDetails_ProjectDetailId] FOREIGN KEY ([ProjectDetailId]) REFERENCES [ProjectDetails] ([Id]) ON DELETE CASCADE
);
GO

CREATE INDEX [IX_TelemarketerCalls_EmployeeId] ON [TelemarketerCalls] ([EmployeeId]);
GO

CREATE INDEX [IX_TelemarketerCalls_ProjectDetailId] ON [TelemarketerCalls] ([ProjectDetailId]);
GO

