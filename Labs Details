BEGIN TRANSACTION;
GO

DECLARE @var0 sysname;
SELECT @var0 = [d].[name]
FROM [sys].[default_constraints] [d]
INNER JOIN [sys].[columns] [c] ON [d].[parent_column_id] = [c].[column_id] AND [d].[parent_object_id] = [c].[object_id]
WHERE ([d].[parent_object_id] = OBJECT_ID(N'[ProjectDetails]') AND [c].[name] = N'Segment');
IF @var0 IS NOT NULL EXEC(N'ALTER TABLE [ProjectDetails] DROP CONSTRAINT [' + @var0 + '];');
ALTER TABLE [ProjectDetails] DROP COLUMN [Segment];
GO

EXEC sp_rename N'[Projects].[TypeId]', N'ProjectTypeId', N'COLUMN';
GO

ALTER TABLE [ProjectDetails] ADD [SegmentName] nvarchar(450) NULL;
GO

CREATE TABLE [ProjectDictionaries] (
    [Id] int NOT NULL IDENTITY,
    [RangFrom] float NOT NULL,
    [RangTo] float NOT NULL,
    [Value] nvarchar(max) NOT NULL,
    [IsDeleted] bit NOT NULL,
    [CreatedBy] nvarchar(max) NOT NULL,
    [AddedOn] datetime2 NOT NULL,
    [LastUpdatedBy] nvarchar(max) NOT NULL,
    [LastUpdatedDate] datetime2 NOT NULL,
    [ProjectId] int NOT NULL,
    CONSTRAINT [PK_ProjectDictionaries] PRIMARY KEY ([Id]),
    CONSTRAINT [FK_ProjectDictionaries_Projects_ProjectId] FOREIGN KEY ([ProjectId]) REFERENCES [Projects] ([Id]) ON DELETE CASCADE
);
GO

CREATE TABLE [ProjectTypes] (
    [Id] int NOT NULL IDENTITY,
    [Name] nvarchar(max) NOT NULL,
    CONSTRAINT [PK_ProjectTypes] PRIMARY KEY ([Id])
);
GO

CREATE TABLE [Segments] (
    [Name] nvarchar(450) NOT NULL,
    [IsDefault] bit NOT NULL,
    [IsDeleted] bit NOT NULL,
    [CreatedBy] nvarchar(max) NOT NULL,
    [AddedOn] datetime2 NOT NULL,
    CONSTRAINT [PK_Segments] PRIMARY KEY ([Name])
);
GO

CREATE TABLE [TypeDictionaries] (
    [Id] int NOT NULL IDENTITY,
    [RangFrom] float NOT NULL,
    [RangTo] float NOT NULL,
    [Value] nvarchar(max) NOT NULL,
    [IsDeleted] bit NOT NULL,
    [CreatedBy] nvarchar(max) NOT NULL,
    [AddedOn] datetime2 NOT NULL,
    [LastUpdatedBy] nvarchar(max) NOT NULL,
    [LastUpdatedDate] datetime2 NOT NULL,
    [ProjectTypeId] int NOT NULL,
    CONSTRAINT [PK_TypeDictionaries] PRIMARY KEY ([Id]),
    CONSTRAINT [FK_TypeDictionaries_ProjectTypes_ProjectTypeId] FOREIGN KEY ([ProjectTypeId]) REFERENCES [ProjectTypes] ([Id]) ON DELETE CASCADE
);
GO

CREATE INDEX [IX_Projects_ProjectTypeId] ON [Projects] ([ProjectTypeId]);
GO

CREATE INDEX [IX_ProjectDetails_SegmentName] ON [ProjectDetails] ([SegmentName]);
GO

CREATE INDEX [IX_ProjectDictionaries_ProjectId] ON [ProjectDictionaries] ([ProjectId]);
GO

CREATE INDEX [IX_TypeDictionaries_ProjectTypeId] ON [TypeDictionaries] ([ProjectTypeId]);
GO

ALTER TABLE [ProjectDetails] ADD CONSTRAINT [FK_ProjectDetails_Segments_SegmentName] FOREIGN KEY ([SegmentName]) REFERENCES [Segments] ([Name]);
GO

ALTER TABLE [Projects] ADD CONSTRAINT [FK_Projects_ProjectTypes_ProjectTypeId] FOREIGN KEY ([ProjectTypeId]) REFERENCES [ProjectTypes] ([Id]) ON DELETE CASCADE;
GO

INSERT INTO [__EFMigrationsHistory] ([MigrationId], [ProductVersion])
VALUES (N'20240911091812_AddSprint4Tables', N'8.0.2');
GO

COMMIT;
GO

